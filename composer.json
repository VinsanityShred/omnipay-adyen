{
    "name": "academe/omnipay-adyen",
    "type": "library",
    "description": "Adyen driver for the Omnipay payment processing library",
    "keywords": [
        "adyen",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/academe/omnipay-adyen",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Judge",
            "email": "jason@academe.co.uk"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-dummy/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Adyen\\" : "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "omnipay/common": "^3.0",
        "symfony/http-client": "^7.3",
        "php-http/httplug": "^2.4",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
